{
  "name": "cql2-filters-parser",
  "description": "A browser-oriented implementation of OGC CQL2 filters in TypeScript",
  "homepage": "https://github.com/NoamRa/ogc-cql2-filters",
  "version": "0.9.1",
  "type": "module",
  "exports": {
    ".": "./dist/cq2-filter-parser.es.js"
  },
  "scripts": {
    "parse": "npx tsx ./src/cql/main.ts",
    "\n========== Building ==========": "",
    "build": "vite build && node ./ci/buildSanity.test.js",
    "dev": "vite",
    "preview": "vite preview",
    "\n========== Linting, Testing, Type checking ==========": "",
    "checks": "npm run lint && npm run type-check && npm run test:once",
    "lint": "eslint .",
    "test:coverage": "vitest run --coverage ./src",
    "test:once": "vitest run ./src",
    "test:ci": "vitest run ./ci",
    "test": "vitest --dir ./src",
    "type-check": "tsc --noEmit",
    "\n========== Publishing ==========": "",
    "publish:dry": "npm publish --dry-run",
    "prepublishOnly": "npm run checks && npm run build"
  },
  "files": [
    "package.json",
    "LICENSE",
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map"
  ],
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^22.13.1",
    "@types/react-dom": "^18.3.0",
    "@types/react": "^18.3.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-istanbul": "^3.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-react": "^7.36.0",
    "eslint": "^9.15.0",
    "globals": "^15.9.0",
    "prettier": "3.3.3",
    "react-dom": "^18.3.1",
    "react": "^18.3.1",
    "typescript-eslint": "^8.16.0",
    "typescript": "5.5.3",
    "vite-plugin-dts": "^4.5.4",
    "vite": "^6.3.5",
    "vitest": "^3.1.1"
  }
}
